
package utils;
public class Math {
    public static int factorial(int n)
    {
        if(n <= 1)
        {
            return 1;
        }
        return n * factorial(n - 1);
    }
    
    public static long factorialLoop(int n)
    {
    	long a=1;
    	for (int in= number; i>1; i--){
    	a=a*i
        }
        return a;
    }